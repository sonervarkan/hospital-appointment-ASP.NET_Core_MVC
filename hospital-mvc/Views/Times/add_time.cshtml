@model hospital_mvc.Models.Entities.Times

<h2>Add a New Time</h2>

<form asp-action="add_time" method="post">
    <div class="form-group">
        <label asp-for="Hour" class="control-label">Hour</label>
        <input asp-for="Hour" class="form-control" />
        <span asp-validation-for="Hour" class="text-danger"></span>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Add</button>
    </div>
</form>